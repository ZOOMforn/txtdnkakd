-- Scripts:

local function QFGXPCF_fake_script() -- MeuFrame.LocalScript 
	local script = Instance.new('LocalScript', MeuFrame)

	 
	local MeuFrame = script.Parent
	local userInputService = game:GetService("UserInputService")
	
	local dragging = false
	local dragOffset = Vector2.new()
	
	MeuFrame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragOffset = Vector2.new(input.Position.X, input.Position.Y) - adminFrame.AbsolutePosition
		end
	end)
	
	MeuFrame.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
	
	userInputService.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			MeuFrame.Position = UDim2.new(0, input.Position.X - dragOffset.X, 0, input.Position.Y - dragOffset.Y)
		end
	end)
end
coroutine.wrap(QFGXPCF_fake_script)()
local function XPTLUP_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	script.Parent.Open.MouseButton1Click:Connect(function()
		script.Parent.APP.Visible = true
	end)
	
	script.Parent.APP.Close.MouseButton1Click:Connect(function()
		script.Parent.APP.Visible = false
	end)
	
	
	
end
coroutine.wrap(XPTLUP_fake_script)()
